<style>
    #site-loader {
        position: fixed;
        top: 0;
        left: 0;
        right: 0;
        background: rgb(var(--color-base-background-1));
        z-index: 100;
        display: flex;
        align-items: center;
        justify-content: center;
        height: 100%;
        overflow: hidden;
    }
    #site-loader img {
        flex: 0 0 auto;
        width: 200px;
    }
</style>
<div id="site-loader">
    <img src="data:image/gif;base64," alt="Pixel Beaker Drawing" aria-hidden="true">
</div>
<script>
    function removeLoader() {
        document.querySelector('#site-loader').style.display = 'none';
    }
    setTimeout(function() {
        removeLoader();
    }, 1100);

    document.addEventListener('readystatechange', function() {
        if (document.readyState === 'complete') {
            removeLoader();
        }
    });
</script>
